.main{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  text-align: center;
  width: 50%;
  height: 50%;
}
.main>view:nth-child(2){
  color: #108EE9;
  padding: 30rpx;
  font-size: 40rpx;
}
.main>view:nth-child(3){
  color: #108EE9;
}

.main button{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
}